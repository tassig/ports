package_name=linux-kernel-headers
package_version=4.4.6
tarball_suffix=xz
url=http://mirrors.tassig.com/kernel/linux-4.4.6.tar.xz
build_dependencies=
iscustombuild=1

custombuild(){
	wget $url
	tar xvf linux-4.4.6.tar.xz
	cd linux-4.4.6
	make INSTALL_HDR_PATH=/opt/$package_name-$package_version/ headers_install
	cd ..
}
